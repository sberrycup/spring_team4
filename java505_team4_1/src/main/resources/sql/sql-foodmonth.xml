<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bitc.java505_team4_1.mapper.FoodMonthMapper">
    <select id="foodMonthList" parameterType="String" resultType="com.bitc.java505_team4_1.dto.FoodMonthDto">
<!--        Select-->
<!--            foodm_name-->
<!--        from-->
<!--            food_months-->
<!--        where-->
<!--            foodm_name = #{foodMName}-->
<!--        and-->
<!--            foodm_month = #{foodMMonth}-->
        select
            foodm_name, foodm_month
        From
            food_months
        Where
            foodm_name like concat('%',#{foodMName},'%')
    </select>

    <select id="foodNameNullMonthList" parameterType="String" resultType="com.bitc.java505_team4_1.dto.FoodMonthDto">
        Select
            foodm_name
        from
            food_months
        where
            foodm_month = #{foodMMonth}
    </select>
</mapper>